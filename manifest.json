{
"manifest_version": 3,
"name": "De Overmolen",
"description": "Shows name of seniors instead of the activity",
"version": "1.0",
"author": "Ahmed Faqiri",
"host_permissions": ["https://*.exactonline.be/*"],
    "content_scripts": [
        {
            "matches": [
                "https://*.exactonline.be/*"
            ],
            "all_frames": true,
            "js": [
                "content.js"
                
            ],
            "run_at": "document_end"
        }
    ]
}